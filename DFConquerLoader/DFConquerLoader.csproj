<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>injection.ico</ApplicationIcon>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Cristian Ocaña Soler (DaRkFoxDeveloper)</Authors>
    <Company>DaRkFoxDeveloper</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Reloaded.Injector" Version="1.2.3" />
  </ItemGroup>

  <ItemGroup>
    <NativeLibs Remove="injection.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="injection.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="injection.ico" />
  </ItemGroup>

</Project>